<!DOCTYPE html>
<html style="height:100%;">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="keywords" content="ruc, dni, pymestudio, developer, peru" />
    <meta name="description" content="Pymestudio IMM CORP" />
    <title>@ViewBag.Title</title>
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,400,300,600,700" rel="stylesheet">
    <link href="http://fonts.googleapis.com/css?family=Lato:300,400,700,300italic,400italic,700italic" rel="stylesheet">
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,400,300,600,700" rel="stylesheet">
    <link href="http://fonts.googleapis.com/css?family=Lato:300,400,700,300italic,400italic,700italic" rel="stylesheet">
    @RenderSection("styles", required: false)
</head>
<body data-spy="scroll" data-target="nav">
    <header id="header-2" class="soft-scroll header-2 header-nav-bar">
        <nav class="main-nav navbar navbar-default navbar-fixed-top">
            <div class="container">
                <!-- Brand and toggle get grouped for better mobile display -->
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-collapse">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a href="@Url.Action("Index", "Home")">
                        <h1 class="asbestos">
                            Pyme Studio
                        </h1>
                    </a>
                </div>
                <!-- Collect the nav links, forms, and other content for toggling -->
                <div class="collapse navbar-collapse" id="navbar-collapse">
                    <ul class="nav navbar-nav navbar-right">
                        <li class="nav-item active">
                            @Html.ActionLink("Inicio", "Index", "Home", new { area = "" }, null)
                        </li>
                        <li class="nav-item">
                            @Html.ActionLink("Api", "Index", "Help", new { area = "" }, null)
                        </li>
                        <li class="nav-item">
                            @Html.ActionLink("Ejemplos", "Example", "Home", new { area = "" }, null)
                        </li>
                        <li class="nav-item">
                            <a href="mailto:giansalex@gmail.com?Subject=Pymestudio">Contacto</a>
                        </li>
                    </ul>
                </div>
                <!-- /.navbar-collapse -->
            </div>
            <!-- /.container-fluid -->
        </nav>
    </header>
    @RenderBody()
    @*<div class="container">
        @RenderBody()
        <hr />

    </div>*@
    <footer>
        <section class="content-block-nopad bg-deepocean">
            <div class="container footer-1-3">
                <div class="row">
                    <div class="col-md-4 pull-left">
                        <h2 style="color: white;">
                            Pyme <b>Studio</b>
                        </h2>
                        <ul class="social social-light">
                            <li>
                                <a href="#"><i class="fa fa-2x fa-facebook"></i></a>
                            </li>
                            <li>
                                <a href="#"><i class="fa fa-2x fa-twitter"></i></a>
                            </li>
                            <li>
                                <a href="#"><i class="fa fa-2x fa-google-plus"></i></a>
                            </li>
                            <li>
                                <a href="#"><i class="fa fa-2x fa-pinterest"></i></a>
                            </li>
                            <li>
                                <a href="#"><i class="fa fa-2x fa-behance"></i></a>
                            </li>
                            <li>
                                <a href="#"><i class="fa fa-2x fa-dribbble"></i></a>
                            </li>
                        </ul>
                        <!-- /.social -->
                    </div>
                    <div class="col-md-3 pull-right">
                        <p class="address-bold-line">Y aun falta más.</p>
                        <p class="address small">
                            Jr. Marte 388,<br>
                            Pueblo Libre,<br>
                            Lima
                        </p>
                    </div> 
                </div>
                <div class="col-xs-12 footer-text">
                    <p>&copy; @DateTime.Now.Year - IMM CORP</p>
                </div>
            </div>
            <!-- /.container -->
        </section>
    </footer>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
